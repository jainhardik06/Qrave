    "eslint": "8.56.0",
    "nx": "17.0.0",
    "typescript": "5.3.3"
  },
  "dependencies": {
    "@nestjs/common": "10.3.0",
    "@nestjs/core": "10.3.0",
    "@nestjs/platform-express": "10.3.0",
    "@nestjs/mongoose": "10.0.1",
    "mongoose": "7.6.3",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.1",
    "tslib": "2.6.2",
    "next": "14.0.3",
