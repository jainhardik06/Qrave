    "@types/react": "18.2.33",
    "@types/react-dom": "18.2.14",
    "eslint": "8.56.0",
    "nx": "^22.1.3",
    "typescript": "5.3.3"
  },
  "dependencies": {
    "@nestjs/common": "^10.4.20",
    "@nestjs/core": "^10.4.20",
    "@nestjs/jwt": "10.2.0",
    "@nestjs/mongoose": "10.0.1",
    "@nestjs/passport": "10.0.3",
    "@nestjs/platform-express": "^10.4.20",
    "bcrypt": "5.1.1",
